{
  "manifest_version": 3,
  "name": "YouTube Style Fixer",
  "version": "1.0",
  "description": "improves the new design of YouTube",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
